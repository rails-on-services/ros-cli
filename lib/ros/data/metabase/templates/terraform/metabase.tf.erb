module "metabase_cards" {
  source               = "rails-on-services/metabase/data"
  backend_host         = "<%= app.api_uri %>"
  backend_user         = "<%= metabase.config.backend_user %>"
  backend_password     = "<%= metabase.config.backend_password %>"
  backend_account_id   = "<%= metabase.config.backend_account_id %>"
  metabase_host        = "<%= metabase.config.metabase_host %>"
  metabase_username    = "<%= metabase.config.metabase_username %>"
  metabase_password    = "<%= metabase.config.metabase_password %>"
  metabase_database_id = "<%= metabase.config.metabase_database_id %>"
  metabase_profile     = "<%= Ros.profile %>"
  metabase_feature_set = "<%= current_feature_set == "master" ? "" : current_feature_set %>"
  data_repo_path       = "./whistler-data-<%= metabase.config.data_version %>"
}